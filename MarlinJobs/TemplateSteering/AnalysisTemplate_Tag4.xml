<marlin>
  <execute>
    <group name="MyFastJetGroup"/>                    <!-- Make some jets -->
    <processor name = "MyJetsToPFOs"/>                <!-- Convert jets to PFOs -->
    <group name="MyVertexFinderGroup"/>               <!-- Find some vertices, not using jet information -->
    <group name="MyJetClusteringGroup"/>              <!-- Cluster some jets, then refine the vertexing using the jet information -->
    <group name="MyJetClusteringAndFlavourTagGroup"/> <!-- Cluster some jets, then refine the vertexing using the jet information and apply flavour tag information-->
    <processor name="MyThrustReconstruction"/>        <!-- Calculate thrust -->
    <processor name="MySphere"/>                      <!-- Calculate sphericity -->
    <processor name="MyAnalysisProcessor"/>           <!-- Run my analysis --> 
    <!--processor name="MyLCIOOutputProcessor"/-->
  </execute>

  <global>
    <parameter name="LCIOInputFiles">
      InputSlcioFile
    </parameter>
    <parameter name="GearXMLFile" value="GearFile"/>
    <parameter name="MaxRecordNumber" value="-1"/>
    <parameter name="SkipNEvents" value="0"/>
    <parameter name="SupressCheck" value="false"/>
    <parameter name="Verbosity" options="DEBUG0-4,MESSAGE0-4,WARNING0-4,ERROR0-4,SILENT"> MESSAGE </parameter>
  </global>

  <group name="MyFastJetGroup">
    <parameter name="recombinationScheme" type="string">E_scheme </parameter>
    <parameter name="recParticleIn" type="string" lcioInType="ReconstructedParticle"> SelectedPandoraPFANewPFOs </parameter>
    <processor name = "MyFastJetProcessor_SelectedPFOs_kt_0p7_6jet" type="FastJetProcessor">
      <parameter name="clusteringMode" type="StringVec">ExclusiveNJets 6</parameter>
      <parameter name="algorithm" type="StringVec">kt_algorithm 0.7 </parameter>
      <parameter name="jetOut" type="string" lcioOutType="ReconstructedParticle"> SelectedPFOs_kt_0p7_6jet_Initial </parameter>
    </processor>
    <processor name = "MyFastJetProcessor_SelectedPFOs_kt_1p0_6jet" type="FastJetProcessor">
      <parameter name="clusteringMode" type="StringVec">ExclusiveNJets 6</parameter>
      <parameter name="algorithm" type="StringVec">kt_algorithm 1.0 </parameter>
      <parameter name="jetOut" type="string" lcioOutType="ReconstructedParticle"> SelectedPFOs_kt_1p0_6jet_Initial </parameter>
    </processor>
    <processor name = "MyFastJetProcessor_SelectedPFOs_kt_0p7_4jet" type="FastJetProcessor">
      <parameter name="clusteringMode" type="StringVec">ExclusiveNJets 4</parameter>
      <parameter name="algorithm" type="StringVec">kt_algorithm 0.7 </parameter>
      <parameter name="jetOut" type="string" lcioOutType="ReconstructedParticle"> SelectedPFOs_kt_0p7_4jet_Initial </parameter>
    </processor>
    <processor name = "MyFastJetProcessor_SelectedPFOs_kt_1p0_4jet" type="FastJetProcessor">
      <parameter name="clusteringMode" type="StringVec">ExclusiveNJets 4</parameter>
      <parameter name="algorithm" type="StringVec">kt_algorithm 1.0 </parameter>
      <parameter name="jetOut" type="string" lcioOutType="ReconstructedParticle"> SelectedPFOs_kt_1p0_4jet_Initial </parameter>
    </processor>
    <processor name = "MyFastJetProcessor_SelectedPFOs_kt_0p7_2jet" type="FastJetProcessor">
      <parameter name="clusteringMode" type="StringVec">ExclusiveNJets 2</parameter>
      <parameter name="algorithm" type="StringVec">kt_algorithm 0.7 </parameter>
      <parameter name="jetOut" type="string" lcioOutType="ReconstructedParticle"> SelectedPFOs_kt_0p7_2jet_Initial </parameter>
    </processor>
    <processor name = "MyFastJetProcessor_SelectedPFOs_kt_1p0_2jet" type="FastJetProcessor">
      <parameter name="clusteringMode" type="StringVec">ExclusiveNJets 2</parameter>
      <parameter name="algorithm" type="StringVec">kt_algorithm 1.0 </parameter>
      <parameter name="jetOut" type="string" lcioOutType="ReconstructedParticle"> SelectedPFOs_kt_1p0_2jet_Initial </parameter>
    </processor>
  </group>

  <processor name="MyJetsToPFOs" type="JetsToPFOs">
    <parameter name="JetInputCollectionNames" type="StringVec"> SelectedPFOs_kt_0p7_6jet_Initial SelectedPFOs_kt_1p0_6jet_Initial SelectedPFOs_kt_0p7_4jet_Initial SelectedPFOs_kt_1p0_4jet_Initial SelectedPFOs_kt_0p7_2jet_Initial SelectedPFOs_kt_1p0_2jet_Initial </parameter>
    <parameter name="PFOCollectionNames" type="StringVec"> SelectedPFOs_kt_0p7_6jet_pfos_Initial SelectedPFOs_kt_1p0_6jet_pfos_Initial SelectedPFOs_kt_0p7_4jet_pfos_Initial SelectedPFOs_kt_1p0_4jet_pfos_Initial SelectedPFOs_kt_0p7_2jet_pfos_Initial SelectedPFOs_kt_1p0_2jet_pfos_Initial </parameter>
  </processor>

  <group name="MyVertexFinderGroup" type="LcfiplusProcessor">
    <!-- run primary and secondary vertex finders -->
    <parameter name="Algorithms" type="stringVec"> PrimaryVertexFinder BuildUpVertex </parameter>
    <parameter name="ReadSubdetectorEnergies" type="int" value="1"/> <!-- true for ILD -->
    <parameter name="UpdateVertexRPDaughters" type="int" value="0"/> <!-- false for non-updative PandoraPFOs -->
    <parameter name="PrintEventNumber" type="int" value="0"/> <!-- 0 for not printing event number, n for printing every n events -->
    <parameter name="MagneticField" type="float" value="4"/>
    <parameter name="BeamSizeX" type="float" value="45.0E-6"/>
    <parameter name="BeamSizeY" type="float" value="1.0E-6"/>
    <parameter name="BeamSizeZ" type="float" value="0.044"/>
    <!-- parameters for primary vertex finder -->
    <parameter name="PrimaryVertexFinder.TrackMaxD0" type="double" value="20." />
    <parameter name="PrimaryVertexFinder.TrackMaxZ0" type="double" value="20." />
    <parameter name="PrimaryVertexFinder.TrackMaxInnermostHitRadius" type="double" value="54" />
    <parameter name="PrimaryVertexFinder.TrackMinVtxFtdHits" type="int" value="1" />
    <parameter name="PrimaryVertexFinder.Chi2Threshold" type="double" value="25." />
    <parameter name="PrimaryVertexFinder.UseBeamConstraint" type="int" value="1" />
    <!-- parameters for secondary vertex finder -->
    <parameter name="BuildUpVertex.TrackMaxD0" type="double" value="10." />
    <parameter name="BuildUpVertex.TrackMaxZ0" type="double" value="20." />
    <parameter name="BuildUpVertex.TrackMinPt" type="double" value="0.1" />
    <parameter name="BuildUpVertex.TrackMaxD0Err" type="double" value="0.1" />
    <parameter name="BuildUpVertex.TrackMaxZ0Err" type="double" value="0.1" />
    <parameter name="BuildUpVertex.TrackMinTpcHits" type="int" value="20" />
    <parameter name="BuildUpVertex.TrackMinFtdHits" type="int" value="1" />
    <parameter name="BuildUpVertex.TrackMinVxdHits" type="int" value="1" />
    <parameter name="BuildUpVertex.TrackMinVxdFtdHits" type="int" value="1" />
    <parameter name="BuildUpVertex.PrimaryChi2Threshold" type="double" value="25." />
    <parameter name="BuildUpVertex.SecondaryChi2Threshold" type="double" value="9." />
    <parameter name="BuildUpVertex.MassThreshold" type="double" value="10." />
    <parameter name="BuildUpVertex.MinDistFromIP" type="double" value="0.3" />
    <parameter name="BuildUpVertex.MaxChi2ForDistOrder" type="double" value="1.0" />
    <parameter name="BuildUpVertex.AssocIPTracks" type="int" value="1" />
    <parameter name="BuildUpVertex.AssocIPTracksMinDist" type="double" value="0." />
    <parameter name="BuildUpVertex.AssocIPTracksChi2RatioSecToPri" type="double" value="2.0" />
    <parameter name="BuildUpVertex.UseV0Selection" type="int" value="1" />
    <!-- specify input collection names -->
    <processor name="VertexFinder_SelectedPFOs_kt_0p7_6jet" type="LcfiplusProcessor">
      <parameter name="PFOCollection" type="string"> SelectedPFOs_kt_0p7_6jet_pfos_Initial </parameter>
      <parameter name="PrimaryVertexCollectionName" type="string"> SelectedPFOs_kt_0p7_6jet_PrimaryVertex </parameter>
      <parameter name="BuildUpVertexCollectionName" type="string"> SelectedPFOs_kt_0p7_6jet_BuildUpVertex </parameter>
      <parameter name="BuildUpVertex.V0VertexCollectionName" type="string"> SelectedPFOs_kt_0p7_6jet_BuildUpVertex_V0 </parameter>
    </processor>
    <processor name="VertexFinder_SelectedPFOs_kt_1p0_6jet" type="LcfiplusProcessor">
      <parameter name="PFOCollection" type="string"> SelectedPFOs_kt_1p0_6jet_pfos_Initial </parameter>
      <parameter name="PrimaryVertexCollectionName" type="string"> SelectedPFOs_kt_1p0_6jet_PrimaryVertex </parameter>
      <parameter name="BuildUpVertexCollectionName" type="string"> SelectedPFOs_kt_1p0_6jet_BuildUpVertex </parameter>
      <parameter name="BuildUpVertex.V0VertexCollectionName" type="string"> SelectedPFOs_kt_1p0_6jet_BuildUpVertex_V0 </parameter>
    </processor>
    <processor name="VertexFinder_SelectedPFOs_kt_0p7_4jet" type="LcfiplusProcessor">
      <parameter name="PFOCollection" type="string"> SelectedPFOs_kt_0p7_4jet_pfos_Initial </parameter>
      <parameter name="PrimaryVertexCollectionName" type="string"> SelectedPFOs_kt_0p7_4jet_PrimaryVertex </parameter>
      <parameter name="BuildUpVertexCollectionName" type="string"> SelectedPFOs_kt_0p7_4jet_BuildUpVertex </parameter>
      <parameter name="BuildUpVertex.V0VertexCollectionName" type="string"> SelectedPFOs_kt_0p7_4jet_BuildUpVertex_V0 </parameter>
    </processor>
    <processor name="VertexFinder_SelectedPFOs_kt_1p0_4jet" type="LcfiplusProcessor">
      <parameter name="PFOCollection" type="string"> SelectedPFOs_kt_1p0_4jet_pfos_Initial </parameter>
      <parameter name="PrimaryVertexCollectionName" type="string"> SelectedPFOs_kt_1p0_4jet_PrimaryVertex </parameter>
      <parameter name="BuildUpVertexCollectionName" type="string"> SelectedPFOs_kt_1p0_4jet_BuildUpVertex </parameter>
      <parameter name="BuildUpVertex.V0VertexCollectionName" type="string"> SelectedPFOs_kt_1p0_4jet_BuildUpVertex_V0 </parameter>
    </processor>
    <processor name="VertexFinder_SelectedPFOs_kt_0p7_2jet" type="LcfiplusProcessor">
      <parameter name="PFOCollection" type="string"> SelectedPFOs_kt_0p7_2jet_pfos_Initial </parameter>
      <parameter name="PrimaryVertexCollectionName" type="string"> SelectedPFOs_kt_0p7_2jet_PrimaryVertex </parameter>
      <parameter name="BuildUpVertexCollectionName" type="string"> SelectedPFOs_kt_0p7_2jet_BuildUpVertex </parameter>
      <parameter name="BuildUpVertex.V0VertexCollectionName" type="string"> SelectedPFOs_kt_0p7_2jet_BuildUpVertex_V0 </parameter>
    </processor>
    <processor name="VertexFinder_SelectedPFOs_kt_1p0_2jet" type="LcfiplusProcessor">
      <parameter name="PFOCollection" type="string"> SelectedPFOs_kt_1p0_2jet_pfos_Initial </parameter>
      <parameter name="PrimaryVertexCollectionName" type="string"> SelectedPFOs_kt_1p0_2jet_PrimaryVertex </parameter>
      <parameter name="BuildUpVertexCollectionName" type="string"> SelectedPFOs_kt_1p0_2jet_BuildUpVertex </parameter>
      <parameter name="BuildUpVertex.V0VertexCollectionName" type="string"> SelectedPFOs_kt_1p0_2jet_BuildUpVertex_V0 </parameter>
    </processor>
  </group>

  <group name="MyJetClusteringGroup" type="LcfiplusProcessor">
    <!-- run primary and secondary vertex finders -->
    <parameter name="Algorithms" type="stringVec"> JetClustering JetVertexRefiner</parameter>
    <!-- general parameters -->
    <parameter name="UseMCP" type="int" value="0" /> <!-- MC info not used -->
    <parameter name="MCPCollection" type="string" value="" /> <!-- not used -->
    <parameter name="MCPFORelation" type="string" value="" /> <!-- not used -->
    <parameter name="ReadSubdetectorEnergies" type="int" value="1"/> <!-- true for ILD -->
    <parameter name="UpdateVertexRPDaughters" type="int" value="0"/> <!-- false for non-updative PandoraPFOs -->
    <parameter name="MagneticField" type="float" value="4"/>
    <!-- jet clustering parameters -->
    <parameter name="JetClustering.NJetsRequested" type="intVec" value="2" />
    <parameter name="JetClustering.YCut" type="doubleVec" value="0." /> <!-- specify 0 if not used -->
    <parameter name="JetClustering.UseMuonID" type="int" value="1" /> <!-- jet-muon ID for jet clustering -->
    <parameter name="JetClustering.VertexSelectionMinimumDistance" type="double" value="0.3" /> <!-- in mm -->
    <parameter name="JetClustering.VertexSelectionMaximumDistance" type="double" value="30." /> <!-- in mm -->
    <parameter name="JetClustering.VertexSelectionK0MassWidth" type="double" value="0.02" /> <!-- in GeV -->
    <parameter name="JetClustering.YAddedForJetVertexVertex" type="double" value="100"/> <!-- add penalty for combining vertices -->
    <parameter name="JetClustering.YAddedForJetLeptonVertex" type="double" value="100"/> <!-- add penalty for combining lepton and vertex -->
    <parameter name="JetClustering.YAddedForJetLeptonLepton" type="double" value="100"/> <!-- add penalty for combining leptons -->
    <!-- vertex refiner parameters -->
    <parameter name="JetVertexRefiner.MinPosSingle" type="double" value="0.3" />
    <parameter name="JetVertexRefiner.MaxPosSingle" type="double" value="30." />
    <parameter name="JetVertexRefiner.MinEnergySingle" type="double" value="1." />
    <parameter name="JetVertexRefiner.MaxAngleSingle" type="double" value="0.5" />
    <parameter name="JetVertexRefiner.MaxSeparationPerPosSingle" type="double" value="0.1" />
    <parameter name="JetVertexRefiner.mind0sigSingle" type="double" value="5." />
    <parameter name="JetVertexRefiner.minz0sigSingle" type="double" value="5." />
    <parameter name="JetVertexRefiner.OneVertexProbThreshold" type="double" value="0.001" />
    <parameter name="JetVertexRefiner.MaxCharmFlightLengthPerJetEnergy" type="double" value="0.1" />
    <processor name="JetClustering_SelectedPFOs_kt_0p7_6jet" type="LcfiplusProcessor">
      <parameter name="PFOCollection" type="string"> SelectedPFOs_kt_0p7_6jet_pfos_Initial </parameter> 
      <parameter name="JetClustering.InputVertexCollectionName" type="string"> SelectedPFOs_kt_0p7_6jet_BuildUpVertex </parameter>  
      <parameter name="JetClustering.OutputJetCollectionName" type="stringVec"> SelectedPFOs_kt_0p7_6jet_VertexJets_Intermediate </parameter> 
      <parameter name="JetVertexRefiner.InputJetCollectionName" type="string"> SelectedPFOs_kt_0p7_6jet_VertexJets_Intermediate </parameter> 
      <parameter name="JetVertexRefiner.OutputJetCollectionName" type="string"> SelectedPFOs_kt_0p7_6jet_Intermediate </parameter>    
      <parameter name="JetVertexRefiner.PrimaryVertexCollectionName" type="string"> SelectedPFOs_kt_0p7_6jet_PrimaryVertex </parameter>    
      <parameter name="JetVertexRefiner.InputVertexCollectionName" type="string"> SelectedPFOs_kt_0p7_6jet_BuildUpVertex </parameter>      
      <parameter name="JetVertexRefiner.V0VertexCollectionName" type="string"> SelectedPFOs_kt_0p7_6jet_BuildUpVertex_V0 </parameter>      
      <parameter name="JetVertexRefiner.OutputVertexCollectionName" type="string"> SelectedPFOs_kt_0p7_6jet_RefinedVertex_Intermediate </parameter>     
      <parameter name="PrimaryVertexCollectionName" type="string"> SelectedPFOs_kt_0p7_6jet_PrimaryVertex </parameter>   
    </processor> 
    <processor name="JetClustering_SelectedPFOs_kt_1p0_6jet" type="LcfiplusProcessor">
      <parameter name="PFOCollection" type="string"> SelectedPFOs_kt_1p0_6jet_pfos_Initial </parameter> 
      <parameter name="JetClustering.InputVertexCollectionName" type="string"> SelectedPFOs_kt_1p0_6jet_BuildUpVertex </parameter> 
      <parameter name="JetClustering.OutputJetCollectionName" type="stringVec"> SelectedPFOs_kt_1p0_6jet_VertexJets_Intermediate </parameter> 
      <parameter name="JetVertexRefiner.InputJetCollectionName" type="string"> SelectedPFOs_kt_1p0_6jet_VertexJets_Intermediate </parameter>    
      <parameter name="JetVertexRefiner.OutputJetCollectionName" type="string"> SelectedPFOs_kt_1p0_6jet_Intermediate </parameter>    
      <parameter name="JetVertexRefiner.PrimaryVertexCollectionName" type="string"> SelectedPFOs_kt_1p0_6jet_PrimaryVertex </parameter>    
      <parameter name="JetVertexRefiner.InputVertexCollectionName" type="string"> SelectedPFOs_kt_1p0_6jet_BuildUpVertex </parameter>      
      <parameter name="JetVertexRefiner.V0VertexCollectionName" type="string"> SelectedPFOs_kt_1p0_6jet_BuildUpVertex_V0 </parameter>      
      <parameter name="JetVertexRefiner.OutputVertexCollectionName" type="string"> SelectedPFOs_kt_1p0_6jet_RefinedVertex_Intermediate </parameter>     
      <parameter name="PrimaryVertexCollectionName" type="string"> SelectedPFOs_kt_1p0_6jet_PrimaryVertex </parameter>   
    </processor> 
    <processor name="JetClustering_SelectedPFOs_kt_0p7_4jet" type="LcfiplusProcessor">
      <parameter name="PFOCollection" type="string"> SelectedPFOs_kt_0p7_4jet_pfos_Initial </parameter> 
      <parameter name="JetClustering.InputVertexCollectionName" type="string"> SelectedPFOs_kt_0p7_4jet_BuildUpVertex </parameter>        
      <parameter name="JetClustering.OutputJetCollectionName" type="stringVec"> SelectedPFOs_kt_0p7_4jet_VertexJets_Intermediate </parameter>      
      <parameter name="JetVertexRefiner.InputJetCollectionName" type="string"> SelectedPFOs_kt_0p7_4jet_VertexJets_Intermediate </parameter>    
      <parameter name="JetVertexRefiner.OutputJetCollectionName" type="string"> SelectedPFOs_kt_0p7_4jet_Intermediate </parameter>    
      <parameter name="JetVertexRefiner.PrimaryVertexCollectionName" type="string"> SelectedPFOs_kt_0p7_4jet_PrimaryVertex </parameter>    
      <parameter name="JetVertexRefiner.InputVertexCollectionName" type="string"> SelectedPFOs_kt_0p7_4jet_BuildUpVertex </parameter>      
      <parameter name="JetVertexRefiner.V0VertexCollectionName" type="string"> SelectedPFOs_kt_0p7_4jet_BuildUpVertex_V0 </parameter>      
      <parameter name="JetVertexRefiner.OutputVertexCollectionName" type="string"> SelectedPFOs_kt_0p7_4jet_RefinedVertex_Intermediate </parameter>     
      <parameter name="PrimaryVertexCollectionName" type="string"> SelectedPFOs_kt_0p7_4jet_PrimaryVertex </parameter>
    </processor>
    <processor name="JetClustering_SelectedPFOs_kt_1p0_4jet" type="LcfiplusProcessor">
      <parameter name="PFOCollection" type="string"> SelectedPFOs_kt_1p0_4jet_pfos_Initial </parameter> 
      <parameter name="JetClustering.InputVertexCollectionName" type="string"> SelectedPFOs_kt_1p0_4jet_BuildUpVertex </parameter> 
      <parameter name="JetClustering.OutputJetCollectionName" type="stringVec"> SelectedPFOs_kt_1p0_4jet_VertexJets_Intermediate </parameter> 
      <parameter name="JetVertexRefiner.InputJetCollectionName" type="string"> SelectedPFOs_kt_1p0_4jet_VertexJets_Intermediate </parameter>
      <parameter name="JetVertexRefiner.OutputJetCollectionName" type="string"> SelectedPFOs_kt_1p0_4jet_Intermediate </parameter>
      <parameter name="JetVertexRefiner.PrimaryVertexCollectionName" type="string"> SelectedPFOs_kt_1p0_4jet_PrimaryVertex </parameter>
      <parameter name="JetVertexRefiner.InputVertexCollectionName" type="string"> SelectedPFOs_kt_1p0_4jet_BuildUpVertex </parameter>
      <parameter name="JetVertexRefiner.V0VertexCollectionName" type="string"> SelectedPFOs_kt_1p0_4jet_BuildUpVertex_V0 </parameter>
      <parameter name="JetVertexRefiner.OutputVertexCollectionName" type="string"> SelectedPFOs_kt_1p0_4jet_RefinedVertex_Intermediate </parameter>
      <parameter name="PrimaryVertexCollectionName" type="string"> SelectedPFOs_kt_1p0_4jet_PrimaryVertex </parameter>
    </processor>
    <processor name="JetClustering_SelectedPFOs_kt_0p7_2jet" type="LcfiplusProcessor">
      <parameter name="PFOCollection" type="string"> SelectedPFOs_kt_0p7_2jet_pfos_Initial </parameter> 
      <parameter name="JetClustering.InputVertexCollectionName" type="string"> SelectedPFOs_kt_0p7_2jet_BuildUpVertex </parameter> 
      <parameter name="JetClustering.OutputJetCollectionName" type="stringVec"> SelectedPFOs_kt_0p7_2jet_VertexJets_Intermediate </parameter> 
      <parameter name="JetVertexRefiner.InputJetCollectionName" type="string"> SelectedPFOs_kt_0p7_2jet_VertexJets_Intermediate </parameter>
      <parameter name="JetVertexRefiner.OutputJetCollectionName" type="string"> SelectedPFOs_kt_0p7_2jet_Intermediate </parameter>
      <parameter name="JetVertexRefiner.PrimaryVertexCollectionName" type="string"> SelectedPFOs_kt_0p7_2jet_PrimaryVertex </parameter>
      <parameter name="JetVertexRefiner.InputVertexCollectionName" type="string"> SelectedPFOs_kt_0p7_2jet_BuildUpVertex </parameter>
      <parameter name="JetVertexRefiner.V0VertexCollectionName" type="string"> SelectedPFOs_kt_0p7_2jet_BuildUpVertex_V0 </parameter>
      <parameter name="JetVertexRefiner.OutputVertexCollectionName" type="string"> SelectedPFOs_kt_0p7_2jet_RefinedVertex_Intermediate </parameter>
      <parameter name="PrimaryVertexCollectionName" type="string"> SelectedPFOs_kt_0p7_2jet_PrimaryVertex </parameter>
    </processor>
    <processor name="JetClustering_SelectedPFOs_kt_1p0_2jet" type="LcfiplusProcessor">
      <parameter name="PFOCollection" type="string"> SelectedPFOs_kt_1p0_2jet_pfos_Initial </parameter> 
      <parameter name="JetClustering.InputVertexCollectionName" type="string"> SelectedPFOs_kt_1p0_2jet_BuildUpVertex </parameter> 
      <parameter name="JetClustering.OutputJetCollectionName" type="stringVec"> SelectedPFOs_kt_1p0_2jet_VertexJets_Intermediate </parameter> 
      <parameter name="JetVertexRefiner.InputJetCollectionName" type="string"> SelectedPFOs_kt_1p0_2jet_VertexJets_Intermediate </parameter>
      <parameter name="JetVertexRefiner.OutputJetCollectionName" type="string"> SelectedPFOs_kt_1p0_2jet_Intermediate </parameter>
      <parameter name="JetVertexRefiner.PrimaryVertexCollectionName" type="string"> SelectedPFOs_kt_1p0_2jet_PrimaryVertex </parameter>
      <parameter name="JetVertexRefiner.InputVertexCollectionName" type="string"> SelectedPFOs_kt_1p0_2jet_BuildUpVertex </parameter>
      <parameter name="JetVertexRefiner.V0VertexCollectionName" type="string"> SelectedPFOs_kt_1p0_2jet_BuildUpVertex_V0 </parameter>
      <parameter name="JetVertexRefiner.OutputVertexCollectionName" type="string"> SelectedPFOs_kt_1p0_2jet_RefinedVertex_Intermediate </parameter>
      <parameter name="PrimaryVertexCollectionName" type="string"> SelectedPFOs_kt_1p0_2jet_PrimaryVertex </parameter>
    </processor>
  </group>

  <group name="MyJetClusteringAndFlavourTagGroup">
    <!-- run primary and secondary vertex finders -->
    <parameter name="Algorithms" type="stringVec"> JetClustering JetVertexRefiner FlavorTag ReadMVA</parameter>
    <!-- general parameters -->
    <parameter name="UseMCP" type="int" value="0" /> <!-- MC info not used -->
    <parameter name="MCPCollection" type="string" value="" /> <!-- not used -->
    <parameter name="MCPFORelation" type="string" value="" /> <!-- not used -->
    <parameter name="ReadSubdetectorEnergies" type="int" value="1"/> <!-- true for ILD -->
    <parameter name="UpdateVertexRPDaughters" type="int" value="0"/> <!-- false for non-updative PandoraPFOs -->
    <parameter name="TrackHitOrdering" type="int" value="0"/> <!-- Track hit ordering: 0=ILD-LOI,SID-DBD, 1=ILD-DBD; CLIC_ILD uses same convension as ILD-LOI -->
    <parameter name="PrintEventNumber" type="int" value="0"/> <!-- 0 for not printing event number, n for printing every n events -->
    <parameter name="MagneticField" type="float" value="4"/>
    <!-- jet clustering parameters -->
    <parameter name="JetClustering.YCut" type="doubleVec" value="0." /> <!-- specify 0 if not used -->
    <parameter name="JetClustering.UseMuonID" type="int" value="1" /> <!-- jet-muon ID for jet clustering -->
    <parameter name="JetClustering.VertexSelectionMinimumDistance" type="double" value="0.3" /> <!-- in mm -->
    <parameter name="JetClustering.VertexSelectionMaximumDistance" type="double" value="30." /> <!-- in mm -->
    <parameter name="JetClustering.VertexSelectionK0MassWidth" type="double" value="0.02" /> <!-- in GeV -->
    <parameter name="JetClustering.YAddedForJetVertexVertex" type="double" value="100"/> <!-- add penalty for combining vertices -->
    <parameter name="JetClustering.YAddedForJetLeptonVertex" type="double" value="100"/> <!-- add penalty for combining lepton and vertex -->
    <parameter name="JetClustering.YAddedForJetLeptonLepton" type="double" value="100"/> <!-- add penalty for combining leptons -->
    <!-- vertex refiner parameters -->
    <parameter name="JetVertexRefiner.MinPosSingle" type="double" value="0.3" />
    <parameter name="JetVertexRefiner.MaxPosSingle" type="double" value="30." />
    <parameter name="JetVertexRefiner.MinEnergySingle" type="double" value="1." />
    <parameter name="JetVertexRefiner.MaxAngleSingle" type="double" value="0.5" />
    <parameter name="JetVertexRefiner.MaxSeparationPerPosSingle" type="double" value="0.1" />
    <parameter name="JetVertexRefiner.mind0sigSingle" type="double" value="5." />
    <parameter name="JetVertexRefiner.minz0sigSingle" type="double" value="5." />
    <parameter name="JetVertexRefiner.OneVertexProbThreshold" type="double" value="0.001" />
    <parameter name="JetVertexRefiner.MaxCharmFlightLengthPerJetEnergy" type="double" value="0.1" />
    <!-- FlavorTag parameters -->
    <parameter name="MakeNtuple.AuxiliaryInfo" type="int" value="-1" />
    <parameter name="FlavorTag.D0ProbFileName" type="string">vtxprob/d0prob_zpole.root</parameter>
    <parameter name="FlavorTag.Z0ProbFileName" type="string">vtxprob/z0prob_zpole.root</parameter>
    <parameter name="FlavorTag.WeightsPrefix" type="string" value="2jets" />
    <parameter name="FlavorTag.BookName" type="string" value="bdt" />
    <parameter name="FlavorTag.PIDAlgo" type="string" value="lcfiplus" />
    <parameter name="FlavorTag.CategoryDefinition1" type="string">nvtx==0</parameter>
